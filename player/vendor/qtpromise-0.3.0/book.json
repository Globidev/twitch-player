{
  "title": "QtPromise",
  "description": "Promises/A+ implementation for Qt/C++",
  "author": "Simon Brunel",
  "gitbook": "3.2.3",
  "root": "docs",
  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "anchorjs",
    "edit-link",
    "expand-active-chapter",
    "ga",
    "github"
  ],
  "pluginsConfig": {
    "anchorjs": {
      "icon": "#",
      "placement": "left",
      "visible": "always"
    },
    "edit-link": {
      "base": "https://github.com/simonbrunel/qtpromise/edit/master/docs"
    },
    "ga": {
      "token": "UA-113899811-1",
      "configuration": "auto"
    },
    "github": {
      "url": "https://github.com/simonbrunel/qtpromise"
    },
    "theme-default": {
      "showLevel": false,
      "styles": {
        "website": "assets/style.css"
      }
    }
  }
}
